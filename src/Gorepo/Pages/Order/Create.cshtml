@page
@model Gorepo.Pages.Order.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>HWZWeChatOrder</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="HWZWeChatOrder.OrderId" class="control-label"></label>
                <input asp-for="HWZWeChatOrder.OrderId" class="form-control" />

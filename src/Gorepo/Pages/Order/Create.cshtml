            </div>
            <div class="form-group">
                <label asp-for="HWZWeChatOrder.OrderAmount" class="control-label"></label>
                <input asp-for="HWZWeChatOrder.OrderAmount" class="form-control" />
                <span asp-validation-for="HWZWeChatOrder.OrderAmount" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HWZWeChatOrder.OrderCode" class="control-label"></label>

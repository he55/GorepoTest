                <span asp-validation-for="HWZWeChatOrder.OrderCode" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="HWZWeChatOrder.IsOrderPay" /> @Html.DisplayNameFor(model => model.HWZWeChatOrder.IsOrderPay)
